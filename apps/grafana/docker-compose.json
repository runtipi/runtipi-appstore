{
  "services": [
    {
      "name": "grafana",
      "image": "grafana/grafana-oss:11.4.0",
      "isMain": true,
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/grafana",
          "containerPath": "/var/lib/grafana"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
