{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "grafana",
      "image": "grafana/grafana-oss:12.3.1",
      "isMain": true,
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/grafana",
          "containerPath": "/var/lib/grafana"
        }
      ]
    }
  ]
}
