{
  "services": [
    {
      "name": "onedev",
      "image": "1dev/server:14.0.7",
      "isMain": true,
      "internalPort": 6610,
      "addPorts": [
        {
          "hostPort": 6611,
          "containerPort": 6611
        }
      ],
      "environment": [
        {
          "key": "hibernate_dialect",
          "value": "io.onedev.server.persistence.PostgreSQLDialect"
        },
        {
          "key": "hibernate_connection_driver_class",
          "value": "org.postgresql.Driver"
        },
        {
          "key": "hibernate_connection_url",
          "value": "jdbc:postgresql://onedev-db:5432/onedev"
        },
        {
          "key": "hibernate_connection_username",
          "value": "tipi"
        },
        {
          "key": "hibernate_connection_password",
          "value": "${ONEDEV_DB_PASSWORD}"
        },
        {
          "key": "initial_user",
          "value": "${ONEDEV_USERNAME}"
        },
        {
          "key": "initial_password",
          "value": "${ONEDEV_PASSWORD}"
        },
        {
          "key": "initial_email",
          "value": "${ONEDEV_EMAIL}"
        },
        {
          "key": "initial_server_url",
          "value": "${APP_DOMAIN}"
        }
      ],
      "dependsOn": ["onedev-db"],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/onedev",
          "containerPath": "/opt/onedev"
        },
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock"
        }
      ]
    },
    {
      "name": "onedev-db",
      "image": "postgres:14",
      "environment": [
        {
          "key": "POSTGRES_USER",
          "value": "tipi"
        },
        {
          "key": "POSTGRES_PASSWORD",
          "value": "${ONEDEV_DB_PASSWORD}"
        },
        {
          "key": "POSTGRES_DB",
          "value": "onedev"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/postgres",
          "containerPath": "/var/lib/postgresql/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
