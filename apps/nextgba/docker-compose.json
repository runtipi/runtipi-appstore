{
  "services": [
    {
      "name": "nextgba",
      "image": "ghcr.io/nicotsx/nextgba:v0.1.0",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "NODE_ENV",
          "value": "production"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/games",
          "containerPath": "/data/games"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
