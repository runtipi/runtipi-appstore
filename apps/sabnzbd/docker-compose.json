{
  "$schema": "../dynamic-compose-schema.json",
  "services": [
    {
      "name": "sabnzbd",
      "image": "lscr.io/linuxserver/sabnzbd:4.4.1",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media/usenet",
          "containerPath": "/media/usenet"
        }
      ]
    }
  ]
}
