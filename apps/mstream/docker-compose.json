{
  "services": [
    {
      "name": "mstream",
      "image": "lscr.io/linuxserver/mstream:v5.12.2-ls190",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/mstream-config",
          "containerPath": "/data"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media/data/music",
          "containerPath": "/music"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
