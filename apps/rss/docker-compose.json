{
  "services": [
    {
      "name": "rss",
      "image": "codeberg.org/danb/rss:v1.6.1",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "APP_NAME",
          "value": "Tipi-RSS"
        },
        {
          "key": "APP_FEED_UPDATE_FREQUENCY",
          "value": "35"
        },
        {
          "key": "APP_LOAD_POST_THUMBNAILS",
          "value": "true"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/storage",
          "containerPath": "/app/storage"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
