{
  "services": [
    {
      "name": "spacedrive",
      "image": "ghcr.io/spacedriveapp/spacedrive/server:0.4.2",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "SD_AUTH": "${SD_AUTH_USER}:${SD_AUTH_PASSWORD}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/spacedrive",
          "containerPath": "/var/spacedrive"
        }
      ]
    }
  ]
}
