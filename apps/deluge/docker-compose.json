{
  "services": [
    {
      "name": "deluge",
      "image": "lscr.io/linuxserver/deluge:2.1.1",
      "isMain": true,
      "internalPort": 8112,
      "addPorts": [
        {
          "hostPort": 6881,
          "containerPort": 6881,
          "tcp": true,
          "udp": true
        }
      ],
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}",
        "DELUGE_LOGLEVEL": "error"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/deluge/config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media/torrents",
          "containerPath": "/media/torrents"
        }
      ]
    }
  ]
}
