{
  "services": [
    {
      "image": "ghcr.io/mealie-recipes/mealie:v3.4.0",
      "name": "mealie-1",
      "internalPort": 9000,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data"
        }
      ],
      "environment": [
        {
          "key": "ALLOW_SIGNUP",
          "value": "${MEALIE_ALLOW_SIGNUP}"
        },
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "MAX_WORKERS",
          "value": "1"
        },
        {
          "key": "WEB_CONCURRENCY",
          "value": "1"
        },
        {
          "key": "BASE_URL",
          "value": "${APP_PROTOCOL:-http}://${APP_DOMAIN}"
        }
      ],
      "deploy": {
        "resources": {
          "limits": {
            "memory": "1000M"
          }
        }
      }
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
