{
  "services": [
    {
      "name": "gandi-livedns",
      "image": "jbbodart/gandi-livedns:latest",
      "isMain": true,
      "environment": [
        {
          "key": "GANDI_PAT",
          "value": "${GANDI_LIVEDNS_PAT}"
        },
        {
          "key": "RECORD_LIST",
          "value": "${GANDI_LIVEDNS_RECORD_LIST}"
        },
        {
          "key": "DOMAIN",
          "value": "${GANDI_LIVEDNS_DOMAIN}"
        },
        {
          "key": "REFRESH_INTERVAL",
          "value": "${GANDI_LIVEDNS_REFRESH_INTERVAL}"
        },
        {
          "key": "TTL",
          "value": "${GANDI_LIVEDNS_TTL}"
        },
        {
          "key": "SET_IPV4",
          "value": "${GANDI_LIVEDNS_SET_IPV4}"
        },
        {
          "key": "SET_IPV6",
          "value": "${GANDI_LIVEDNS_SET_IPV6}"
        },
        {
          "key": "FORCE_IPV4",
          "value": "${GANDI_LIVEDNS_FORCE_IPV4}"
        },
        {
          "key": "FORCE_IPV6",
          "value": "${GANDI_LIVEDNS_FORCE_IPV6}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
