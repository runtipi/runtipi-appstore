{
  "services": [
    {
      "name": "flightlog",
      "image": "perdian/flightlog:v2.1.1",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "FLIGHTLOG_SERVER_CONTEXT_PATH": "/"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/db",
          "containerPath": "/var/flightlog/database"
        }
      ]
    }
  ]
}
