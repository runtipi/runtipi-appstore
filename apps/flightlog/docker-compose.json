{
  "services": [
    {
      "name": "flightlog",
      "image": "perdian/flightlog:v2.1.11",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "FLIGHTLOG_SERVER_CONTEXT_PATH",
          "value": "/"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/db",
          "containerPath": "/var/flightlog/database"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
