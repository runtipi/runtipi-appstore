{
  "services": [
    {
      "name": "jellyseerr",
      "image": "fallenbagel/jellyseerr:2.7.3",
      "isMain": true,
      "internalPort": 5055,
      "environment": [
        {
          "key": "LOG_LEVEL",
          "value": "debug"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/jellyseerr-config",
          "containerPath": "/app/config"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
