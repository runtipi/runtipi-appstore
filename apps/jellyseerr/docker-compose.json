{
  "services": [
    {
      "name": "jellyseerr",
      "image": "fallenbagel/jellyseerr:2.1.0",
      "isMain": true,
      "internalPort": 5055,
      "environment": {
        "LOG_LEVEL": "debug",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/jellyseerr-config",
          "containerPath": "/app/config"
        }
      ]
    }
  ]
}
