{
  "services": [
    {
      "name": "flatnotes",
      "image": "dullage/flatnotes:v5.5.4",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "FLATNOTES_AUTH_TYPE",
          "value": "${FLATNOTES_AUTH_TYPE}"
        },
        {
          "key": "FLATNOTES_USERNAME",
          "value": "${FLATNOTES_USERNAME}"
        },
        {
          "key": "FLATNOTES_PASSWORD",
          "value": "${FLATNOTES_PASSWORD}"
        },
        {
          "key": "FLATNOTES_SECRET_KEY",
          "value": "${FLATNOTES_SECRET_KEY}"
        },
        {
          "key": "FLATNOTES_TOTP_KEY",
          "value": "${FLATNOTES_TOTP_KEY}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
