{
  "services": [
    {
      "name": "freshrss",
      "image": "lscr.io/linuxserver/freshrss:1.25.0",
      "isMain": true,
      "internalPort": 80,
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/freshrss",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
