{
  "services": [
    {
      "image": "ghcr.io/autobrr/autobrr:v1.68.0",
      "name": "autobrr",
      "internalPort": 7474,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/autobrr",
          "containerPath": "/config"
        }
      ],
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "user": "1000:1000"
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
