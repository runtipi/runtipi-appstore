{
  "$schema": "../dynamic-compose-schema.json",
  "services": [
    {
      "name": "cup",
      "image": "ghcr.io/sergi0g/cup:v3.2.2",
      "command": "serve",
      "volumes": [
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock"
        }
      ],
      "internalPort": 8000,
      "isMain": true
    }
  ]
}
