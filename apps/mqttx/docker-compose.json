{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "mqttx",
      "image": "emqx/mqttx-web:v1.12.1",
      "isMain": true,
      "internalPort": 80,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/mqttx",
          "containerPath": "/app/data"
        },
        {
          "hostPath": "/etc/localtime",
          "containerPath": "/etc/localtime",
          "readOnly": true
        }
      ]
    }
  ]
}
