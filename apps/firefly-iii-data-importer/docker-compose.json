{
  "services": [
    {
      "name": "firefly-iii-data-importer",
      "image": "fireflyiii/data-importer:version-1.8.4",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "FIREFLY_III_URL",
          "value": "${FIREFLY_III_URL}"
        },
        {
          "key": "FIREFLY_III_ACCESS_TOKEN",
          "value": "${FIREFLY_III_ACCESS_TOKEN}"
        },
        {
          "key": "FIREFLY_III_CLIENT_ID",
          "value": "${FIREFLY_III_CLIENT_ID}"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "TRUSTED_PROXIES",
          "value": "**"
        },
        {
          "key": "VERIFY_TLS_SECURITY",
          "value": "false"
        },
        {
          "key": "APP_ENV",
          "value": "local"
        },
        {
          "key": "APP_DEBUG",
          "value": "false"
        },
        {
          "key": "LOG_CHANNEL",
          "value": "stack"
        },
        {
          "key": "LOG_LEVEL",
          "value": "info"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
