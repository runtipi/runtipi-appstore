{
  "services": [
    {
      "name": "mind",
      "image": "mrcas/mind:v1.4.1",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/mind-db",
          "containerPath": "/app/db"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
