{
  "services": [
    {
      "name": "spoolman",
      "image": "ghcr.io/donkie/spoolman:0.22.1",
      "isMain": true,
      "internalPort": 8000,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "PUID",
          "value": "0"
        },
        {
          "key": "PGID",
          "value": "0"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/home/app/.local/share/spoolman"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
