{
  "services": [
    {
      "name": "owncast",
      "image": "owncast/owncast:0.2.1",
      "isMain": true,
      "internalPort": 8080,
      "addPorts": [
        {
          "hostPort": 1935,
          "containerPort": 1935
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
