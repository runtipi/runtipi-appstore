{
  "services": [
    {
      "name": "maintainerr",
      "image": "ghcr.io/jorenn92/maintainerr:2.19.0",
      "isMain": true,
      "internalPort": 6246,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/opt/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
