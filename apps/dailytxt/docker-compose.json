{
  "services": [
    {
      "name": "dailytxt",
      "image": "phitux/dailytxt:1.0.15",
      "isMain": true,
      "internalPort": 8765,
      "environment": [
        {
          "key": "PORT",
          "value": "8765"
        },
        {
          "key": "SECRET_KEY",
          "value": "${DAILYTXT_SECRET_KEY}"
        },
        {
          "key": "ALLOW_REGISTRATION",
          "value": "${DAILYTXT_ALLOW_REGISTRATION}"
        },
        {
          "key": "DATA_INDENT",
          "value": "2"
        },
        {
          "key": "JWT_EXP_DAYS",
          "value": "60"
        },
        {
          "key": "ENABLE_UPDATE_CHECK",
          "value": "True"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/dailytxt",
          "containerPath": "/app/data/"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
