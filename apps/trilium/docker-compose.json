{
  "services": [
    {
      "name": "trilium",
      "image": "ghcr.io/triliumnext/trilium:v0.99.2",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "TRILIUM_DATA_DIR": "/home/node/trilium-data"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/home/node/trilium-data"
        }
      ]
    }
  ],
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
