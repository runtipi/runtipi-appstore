{
  "services": [
    {
      "name": "trilium",
      "image": "ghcr.io/triliumnext/trilium:v0.98.0",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "TRILIUM_DATA_DIR": "/home/node/trilium-data"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/home/node/trilium-data"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
