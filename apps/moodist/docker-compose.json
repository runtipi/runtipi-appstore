{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "moodist",
      "image": "ghcr.io/remvze/moodist:v2.4.0",
      "isMain": true,
      "internalPort": 8080,
      "volumes": [
        {
          "hostPath": "/etc/localtime",
          "containerPath": "/etc/localtime",
          "readOnly": true
        }
      ]
    }
  ]
}
