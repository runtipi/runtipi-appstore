{
  "services": [
    {
      "name": "vaultwarden",
      "image": "vaultwarden/server:1.34.3",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "WEBSOCKET_ENABLED",
          "value": "true"
        },
        {
          "key": "ADMIN_TOKEN",
          "value": "${VAULTWARDEN_ADMIN_PASSWORD}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
