{
  "services": [
    {
      "name": "vaultwarden",
      "image": "vaultwarden/server:1.32.6",
      "isMain": true,
      "internalPort": 80,
      "environment": {
        "WEBSOCKET_ENABLED": "true",
        "ADMIN_TOKEN": "${VAULTWARDEN_ADMIN_PASSWORD}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ]
}
