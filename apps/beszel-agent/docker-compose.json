{
  "services": [
    {
      "name": "beszel-agent",
      "image": "henrygd/beszel-agent:0.15.2",
      "isMain": true,
      "internalPort": 45876,
      "networkMode": "host",
      "environment": [
        {
          "key": "PORT",
          "value": 45876
        },
        {
          "key": "KEY",
          "value": "${BESZEL_AGENT_KEY}"
        }
      ],
      "volumes": [
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock",
          "readOnly": true
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
