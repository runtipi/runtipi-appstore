{
  "services": [
    {
      "name": "beszel-agent",
      "image": "henrygd/beszel-agent:0.8.0",
      "isMain": true,
      "networkMode": "host",
      "environment": {
        "PORT": 45876,
        "KEY": "${BESZEL_AGENT_KEY}"
      },
      "volumes": [
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock",
          "readOnly": true
        }
      ]
    }
  ]
}
