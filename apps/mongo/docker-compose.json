{
  "services": [
    {
      "name": "mongo",
      "image": "mongo:7.0.28",
      "isMain": true,
      "internalPort": 27017,
      "environment": [
        {
          "key": "MONGO_INITDB_ROOT_USERNAME",
          "value": "${MONGO_INITDB_ROOT_USERNAME}"
        },
        {
          "key": "MONGO_INITDB_ROOT_PASSWORD",
          "value": "${MONGO_INITDB_ROOT_PASSWORD}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
