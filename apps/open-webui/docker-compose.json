{
  "services": [
    {
      "image": "ghcr.io/open-webui/open-webui:0.7.2",
      "name": "open-webui",
      "internalPort": 8080,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/backend/data"
        }
      ],
      "environment": [
        {
          "key": "OLLAMA_BASE_URL",
          "value": "${OPENWEBUI_OLLAMA_URL}"
        },
        {
          "key": "OPENAI_API_KEY",
          "value": "${OPENWEBUI_OPENAI_KEY}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
