{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "ollama-cpu",
      "image": "ollama/ollama:0.14.3",
      "isMain": true,
      "internalPort": 11434,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/.ollama",
          "containerPath": "/root/.ollama"
        }
      ]
    }
  ]
}
