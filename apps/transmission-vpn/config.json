      "env_variable": "TRANSMISSION_BLOCKLIST_ENABLED"
    },
    {
      "type": "text",
      "label": "Transmission > Blocklist URL",
      "placeholder": "http://list.iblocklist.com/?list=bt_level1&fileformat=p2p&archiveformat=gz",
