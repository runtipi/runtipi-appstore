{
    "services": [
        {
            "name": "beszel",
            "image": "henrygd/beszel:0.6.2",
            "isMain": true,
            "internalPort": 8090,
            "restart": "unless-stopped",
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/beszel_data"
                }
            ]
        }
    ]
}