{
  "services": [
    {
      "name": "emulatorjs",
      "image": "lscr.io/linuxserver/emulatorjs:1.9.2",
      "isMain": true,
      "internalPort": 80,
      "addPorts": [
        {
          "hostPort": 8165,
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/emulatorjs-config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${APP_DATA_DIR}/data/emulatorjs",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
