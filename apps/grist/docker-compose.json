{
  "services": [
    {
      "name": "grist",
      "image": "gristlabs/grist:1.7.9",
      "isMain": true,
      "internalPort": 8484,
      "environment": [
        {
          "key": "APP_HOME_URL",
          "value": "${APP_PROTOCOL:-http}://${APP_DOMAIN}"
        },
        {
          "key": "GRIST_SANDBOX_FLAVOR",
          "value": "${GRIST_SANDBOX_FLAVOR}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/grist-data",
          "containerPath": "/persist"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
