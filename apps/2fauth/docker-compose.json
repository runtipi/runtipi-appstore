{
  "services": [
    {
      "name": "2fauth",
      "image": "2fauth/2fauth:5.6.0",
      "internalPort": 8000,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/2fauth"
        }
      ],
      "environment": {
        "ASSET_URL": "${APP_PROTOCOL:-http}://${APP_DOMAIN}",
        "APP_URL": "${APP_PROTOCOL:-http}://${APP_DOMAIN}"
      }
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
