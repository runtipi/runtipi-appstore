{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "nodered",
      "image": "nodered/node-red:4.1.3",
      "isMain": true,
      "internalPort": 1880,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ]
}
