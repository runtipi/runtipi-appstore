{
  "services": [
    {
      "name": "dozzle",
      "image": "amir20/dozzle:v8.14.6",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "DOZZLE_NO_ANALYTICS",
          "value": "${DOZZLE_NO_ANALYTICS:-true}"
        },
        {
          "key": "DOZZLE_AUTH_PROVIDER",
          "value": "${DOZZLE_AUTH_PROVIDER:-none}"
        },
        {
          "key": "DOZZLE_ENABLE_ACTIONS",
          "value": "${DOZZLE_ENABLE_ACTIONS:-true}"
        }
      ],
      "volumes": [
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock"
        },
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
