{
  "services": [
    {
      "image": "ghcr.io/joshuaavalon/cloudflare-ddns:3.3.0",
      "name": "cloudflare-ddns",
      "isMain": true,
      "environment": [
        {
          "key": "CF_DNS__AUTH__SCOPED_TOKEN",
          "value": "${CF_DNS__AUTH__SCOPED_TOKEN}"
        },
        {
          "key": "CF_DNS__DOMAINS_0__NAME",
          "value": "${CF_DNS__DOMAINS_0__NAME}"
        },
        {
          "key": "CF_DNS__DOMAINS_0__PROXIED",
          "value": "false"
        },
        {
          "key": "CF_DNS__LOG_TYPE",
          "value": "json"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
