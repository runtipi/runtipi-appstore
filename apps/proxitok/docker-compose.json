{
  "services": [
    {
      "name": "proxitok",
      "image": "ghcr.io/pablouser1/proxitok:master",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "LATTE_CACHE",
          "value": "/cache"
        },
        {
          "key": "API_CACHE",
          "value": "redis"
        },
        {
          "key": "REDIS_HOST",
          "value": "proxitok-redis"
        },
        {
          "key": "REDIS_PORT",
          "value": "6379"
        },
        {
          "key": "API_CHROMEDRIVER",
          "value": "http://proxitok-chromedriver:9515"
        }
      ],
      "dependsOn": ["proxitok-redis", "proxitok-chromedriver"],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/cache",
          "containerPath": "/cache"
        }
      ]
    },
    {
      "name": "proxitok-redis",
      "image": "redis:7.4-alpine",
      "command": "redis-server --save 60 1 --loglevel warning"
    },
    {
      "name": "proxitok-chromedriver",
      "image": "zenika/alpine-chrome:with-chromedriver"
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
