{
  "$schema": "../dynamic-compose-schema.json",
  "services": [
    {
      "name": "kasm-workspaces",
      "image": "lscr.io/linuxserver/kasm:1.120.20221218",
      "isMain": true,
      "internalPort": "${APP_PORT}",
      "addPorts": [
        {
          "hostPort": 8743,
          "containerPort": 3000
        }
      ],
      "environment": {
        "KASM_PORT": "${APP_PORT}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/opt"
        }
      ],
      "privileged": true
    }
  ]
}
