{
  "services": [
    {
      "name": "kasm-workspaces",
      "image": "lscr.io/linuxserver/kasm:1.120.20221218",
      "isMain": true,
      "internalPort": "${APP_PORT}",
      "addPorts": [
        {
          "hostPort": 8743,
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "key": "KASM_PORT",
          "value": "${APP_PORT}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/opt"
        }
      ],
      "privileged": true
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
