{
  "services": [
    {
      "name": "barrage",
      "image": "maulik9898/barrage:0.3.0",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "NEXTAUTH_SECRET",
          "value": "${NEXTAUTH_SECRET}"
        },
        {
          "key": "DELUGE_URL",
          "value": "${DELUGE_URL}"
        },
        {
          "key": "DELUGE_PASSWORD",
          "value": "${DELUGE_PASSWORD}"
        },
        {
          "key": "BARRAGE_PASSWORD",
          "value": "${BARRAGE_PASSWORD}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
