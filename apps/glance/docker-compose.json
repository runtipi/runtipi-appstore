{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "image": "glanceapp/glance:v0.8.4",
      "name": "glance",
      "internalPort": 8080,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/",
          "containerPath": "/app/config/"
        }
      ]
    }
  ]
}
