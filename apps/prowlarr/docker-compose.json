{
  "services": [
    {
      "name": "prowlarr",
      "image": "ghcr.io/linuxserver/prowlarr:1.30.2",
      "isMain": true,
      "internalPort": 9696,
      "environment": {
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
