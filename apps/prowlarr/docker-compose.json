{
  "services": [
    {
      "name": "prowlarr",
      "image": "ghcr.io/linuxserver/prowlarr:2.3.0",
      "isMain": true,
      "internalPort": 9696,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
