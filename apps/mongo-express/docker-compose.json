{
  "services": [
    {
      "name": "mongo-express",
      "image": "mongo-express:1.0.2",
      "isMain": true,
      "internalPort": 8081,
      "environment": [
        {
          "key": "ME_CONFIG_MONGODB_SERVER",
          "value": "${ME_CONFIG_MONGODB_SERVERIP}"
        },
        {
          "key": "ME_CONFIG_MONGODB_ENABLE_ADMIN",
          "value": "true"
        },
        {
          "key": "ME_CONFIG_MONGODB_ADMINUSERNAME",
          "value": "${ME_CONFIG_MONGODB_ADMINUSERNAME}"
        },
        {
          "key": "ME_CONFIG_MONGODB_ADMINPASSWORD",
          "value": "${ME_CONFIG_MONGODB_ADMINPASSWORD}"
        },
        {
          "key": "ME_CONFIG_BASICAUTH_USERNAME",
          "value": "${ME_CONFIG_BASICAUTH_USERNAME}"
        },
        {
          "key": "ME_CONFIG_BASICAUTH_PASSWORD",
          "value": "${ME_CONFIG_BASICAUTH_PASSWORD}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
