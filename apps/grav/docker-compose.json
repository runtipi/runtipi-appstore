{
  "services": [
    {
      "name": "grav",
      "image": "lscr.io/linuxserver/grav:1.7.50",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/grav-comfig",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
