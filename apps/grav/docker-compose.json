{
  "services": [
    {
      "name": "grav",
      "image": "lscr.io/linuxserver/grav:1.7.50",
      "isMain": true,
      "internalPort": 80,
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/grav-comfig",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
