{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "changedetection",
      "image": "ghcr.io/dgtlmoon/changedetection.io:0.50.38",
      "isMain": true,
      "internalPort": 5000,
      "hostname": "changedetection",
      "volumes": [
        {
