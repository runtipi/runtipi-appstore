{
    "services": [
        {
            "name": "changedetection",
            "image": "ghcr.io/dgtlmoon/changedetection.io:0.47.03",
            "isMain": true,
            "internalPort": 5000,
            "hostname": "changedetection",
            "restart": "unless-stopped",
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/datastore"
                }
            ]
        }
    ]
}