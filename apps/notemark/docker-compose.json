{
  "services": [
    {
      "name": "notemark",
      "image": "ghcr.io/enchant97/note-mark-frontend:0.16.3"
    },
    {
      "name": "notemark-backend",
      "image": "ghcr.io/enchant97/note-mark-backend:0.18.0",
      "environment": [
        {
          "key": "JWT_SECRET",
          "value": "${NOTEMARK_SERVICE_SECRET}"
        },
        {
          "key": "CORS_ORIGINS",
          "value": "*"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    },
    {
      "name": "notemark-proxy",
      "image": "ghcr.io/enchant97/note-mark-frontend:0.18.0",
      "isMain": true,
      "internalPort": 80,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/proxy/nginx.conf",
          "containerPath": "/etc/nginx/conf.d/default.conf",
          "readOnly": true
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
