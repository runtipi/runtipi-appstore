{
  "services": [
    {
      "name": "email-oauth2-proxy",
      "image": "ghcr.io/blacktirion/email-oauth2-proxy-docker:2026.01.06",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "DEBUG",
          "value": "true"
        },
        {
          "key": "CACHE_STORE",
          "value": "/config/credstore.config"
        },
        {
          "key": "LOGFILE",
          "value": "true"
        },
        {
          "key": "LOCAL_SERVER_AUTH",
          "value": "true"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
