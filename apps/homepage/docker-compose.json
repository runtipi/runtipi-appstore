{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "homepage",
      "image": "ghcr.io/gethomepage/homepage:v1.5.0",
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/:/app/config",
          "containerPath": "/app/config"
        }
      ],
      "isMain": true
    }
  ]
}
