{
  "services": [
    {
      "name": "suwayomi",
      "image": "ghcr.io/suwayomi/tachidesk:v2.1.1985",
      "isMain": true,
      "internalPort": 4567,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "BIND_IP",
          "value": "0.0.0.0"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/home/suwayomi/.local/share/Tachidesk"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json"
}
