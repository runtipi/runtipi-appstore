{
  "services": [
    {
      "name": "duplicati",
      "image": "lscr.io/linuxserver/duplicati:2.2.0",
      "internalPort": 8200,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/duplicati-config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}",
          "containerPath": "/backups/runtipi-folder"
        },
        {
          "hostPath": "/home",
          "containerPath": "/source"
        }
      ],
      "environment": [
        {
          "key": "PUID",
          "value": "0"
        },
        {
          "key": "PGID",
          "value": "0"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "SETTINGS_ENCRYPTION_KEY",
          "value": "${DUPLICATI_ENCRYPTION_KEY}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
